curl -X POST http://localhost:3000/create/user -H "Authorization: Bearer " --data '{ "username": "Justine1", "password": "Justine1234"}'
curl -X POST http://localhost:3000/create/post -H "Authorization: Bearer " --data '{ "username": "Justine1", "post": "Lorem ipsum dolor sit amet.", "tag": "General"}'
curl -X POST http://localhost:3000/create/tag -H "Authorization: Bearer " --data '{ "tag": "General2"}'

curl -X "DELETE" -H "Authorization: Bearer " http://localhost:3000/delete/user/1
curl -X "DELETE" -H "Authorization: Bearer " http://localhost:3000/delete/post/1
curl -X "DELETE" -H "Authorization: Bearer " http://localhost:3000/delete/tag/1

curl -i -H "Accept: application/json" -H "Content-Type: application/json" -H "Authorization: Bearer " http://localhost:3000/list/user
curl -i -H "Accept: application/json" -H "Content-Type: application/json" -H "Authorization: Bearer " http://localhost:3000/list/post
curl -i -H "Accept: application/json" -H "Content-Type: application/json" -H "Authorization: Bearer " http://localhost:3000/list/tag

curl -H "Authorization: Bearer " --data '{ "username": "Justine23", "password": "Justine1234"}' -X PUT http://localhost:3000/update/user/1
curl -H "Authorization: Bearer " --data '{ "username": "Justine23", "post": "Lorem ipsum dolor sit amet.", "tag": "General23"}' -X PUT http://localhost:3000/update/post/1
curl -H "Authorization: Bearer " --data '{ "tag": "General10"}' -X PUT http://localhost:3000/update/tag/1

curl -X POST http://localhost:3000/login --data '{ "username": "Justine1", "password": "Justine1234"}'

